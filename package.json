{
  "name": "investment-portfolio-tracker",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "description": "A modern, feature-rich web application for tracking and analyzing investment portfolios with advanced bond analysis, multi-currency support, and real-time data integration.",
  "repository": {
    "type": "git",
